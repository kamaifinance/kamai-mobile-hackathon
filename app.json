{
  "expo": {
    "name": "kamai-mobile",
    "slug": "kamai-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.solana.mobile.expo.template"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.solana.mobile.expo.template",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Geist-Regular.ttf",
            "./assets/fonts/Geist-Bold.ttf",
            "./assets/fonts/Geist-Medium.ttf",
            "./assets/fonts/Geist-SemiBold.ttf",
            "./assets/fonts/Geist-Light.ttf",
            "./assets/fonts/Geist-Black.ttf",
            "./assets/fonts/Geist-BlackItalic.ttf",
            "./assets/fonts/Geist-ExtraBold.ttf",
            "./assets/fonts/Geist-ExtraBoldItalic.ttf",
            "./assets/fonts/Geist-ExtraLight.ttf",
            "./assets/fonts/Geist-ExtraLightItalic.ttf",
            "./assets/fonts/Geist-Italic.ttf",
            "./assets/fonts/Saleha.ttf",
            "./assets/fonts/Larken Bold Italic.ttf",
            "./assets/fonts/Larken Bold.ttf",
            "./assets/fonts/Larken Italic.ttf",
            "./assets/fonts/Larken Light Italic.ttf",
            "./assets/fonts/Larken Light.ttf",
            "./assets/fonts/Larken Medium Italic.ttf",
            "./assets/fonts/Larken Medium.ttf",
            "./assets/fonts/Larken Regular.ttf",
            "./assets/fonts/Larken Thin Italic.ttf",
            "./assets/fonts/Larken Thin.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos to your device."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7401ed0e-e101-4762-986f-7574f4add90e"
      }
    }
  }
}
